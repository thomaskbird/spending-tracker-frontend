    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "npm-font-open-sans": "1.1.0",
    "numeral": "2.0.6",
    "openapi-spec-builder": "3.0.0",
    "query-string": "5.1.1",
    "raf": "3.4.0",
    "react": "16.4.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-dom": "16.4.2",
    "react-dom-factories": "1.0.2",
    "react-localize-redux": "2.17.5",
    "react-redux": "5.0.7",
    "react-redux-typescript": "3.0.0-rc.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-select": "1.3.0",
    "react-transition-group": "2.4.0",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.5",
    "redux-logger": "3.0.6",
    "redux-observable": "0.19.0",
    "rxjs": "6.2.2",
    "shortid": "2.2.12",
    "ts-enum-util": "3.1.0",
    "ts-string-visitor": "2.1.4",
    "url": "0.11.0"
  },
  "scripts": {
    "npm-install": "npm install",
    "npm-reinstall": "rm -rf node_modules && npm run npm-install",
    "npm-check-updates": "ncu --packageFile package.json",
    "npm-update": "rm -f package-lock.json && npm run npm-reinstall",
    "start": "webpack-dev-server --inline --open --port 8000",
    "start-sandbox": "webpack-dev-server --inline --open --env.sandbox --port 9000",
    "compile": "tsc --version && tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
    "compile-for-coverity": "rm -rf out && tsc --version && tsc --project tsconfig.json --sourceMap false --target ES6 --module CommonJS",
    "compile-tslint-rules": "rm -rf tslint-rules/out && tsc --version && tsc --project tslint-rules/tsconfig.json",
    "prettier": "prettier --list-different \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "prettier-fix": "prettier --write \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "tslint --version && tslint --project tsconfig.json --config tslint.json",
    "lint-fix": "tslint --version && tslint --fix --project tsconfig.json --config tslint.json",
    "lint-sass": "sass-lint --version && sass-lint 'src/**/*.scss' -v -q",
    "test": "jest --version && jest",
    "test-coverage": "rm -rf coverage && jest --version && jest --coverage",
    "test-changes": "jest --onlyChanged",
    "test-watch-changes": "jest --watch --onlyChanged",
    "precommit": "npm run compile && npm run lint && npm run lint-sass && npm run prettier && npm run test-coverage",
    "precommit-fix": "npm run compile && npm run lint-fix && npm run lint-sass && npm run prettier-fix && npm run test-coverage",
    "clean": "rm -rf dist",
    "buildProd": "npm run clean && webpack --env.prod",
    "buildProdTest": "npm run clean && webpack --env.prod --env.sourceMap",
    "build-sandbox": "npm run clean && webpack --env.prod --env.sourceMap --env.sandbox",
    "zip-dist": "zip -r dist.zip dist",
    "build-api-code-generator": "mvn -f swagger-codegen/Generator/pom.xml clean package",
    "generate-api-code:main": "java -cp swagger-codegen/swagger-codegen-cli.jar:swagger-codegen/Generator/target/PDSWebApp-swagger-codegen-1.0.0.jar io.swagger.codegen.Codegen -l PDSWebApp -c swagger-codegen/api-docs/main-config.json -i swagger-codegen/api-docs/main.json -o ./src/generated-code/api",
    "generate-api-code:vehicle": "java -cp swagger-codegen/swagger-codegen-cli.jar:swagger-codegen/Generator/target/PDSWebApp-swagger-codegen-1.0.0.jar io.swagger.codegen.Codegen -l PDSWebApp -c swagger-codegen/api-docs/vehicle-config.json -i swagger-codegen/api-docs/vehicle.json -o ./src/generated-code/api",
    "generate-api-code": "rm -rf src/generated-code/api && npm run generate-api-code:main && npm run generate-api-code:vehicle",
    "bamboo-test": "npm run compile && npm run lint && npm run lint-sass && npm run prettier && npm run test-coverage",
    "static-analysis-sonarqube": "sonar-scanner -Dsonar.branch=`git symbolic-ref --short -q HEAD | tr -d '\n'`"
  },
  "jest": {
    "testURL": "http://localhost",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(s?css)$": "<rootDir>/__mocks__/styleMock.js",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/generated-code/**/*.*",
      "!src/**/*.d.ts",
      "!src/sandbox/**/*.*",
      "!src/index.tsx",
      "!src/sandbox.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/jest.json"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/jest-bamboo-formatter"
  }
}
